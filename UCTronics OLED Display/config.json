{
    "name": "UCTRONICS OLED Display",
    "version": "2021.10.03.050",
    "slug": "uctronics_oled_display",
    "description": "This addon lets you display to your OLED provided in the UCTRONICS Pi Rack 4 with the ssd1306 OLED.",
    "arch": [
        "armhf",
        "armv7",
        "aarch64"
    ],
    "startup": "services",
    "auth_api": false,
    "ingress": false,
    "app_armor": true,
    "host_network": true,
    "privileged": [
        "SYS_ADMIN"
    ],
    "devices": [
        "/dev/i2c-1"
    ],
    "gpio": true,
    "stage": "experimental",
    "full_access": true,
    "url": "https://github.com/garethcheyne/HomeAssistant/tree/main/UCTronics%20OLED%20Display",
    "boot": "auto",
    "options": {
        "temperature_unit": "C",
        "stop_auto_run": false,
        "run_dev": false
    },
    "schema": {
        "temperature_unit": "list(C|F)?",
        "stop_auto_run": "bool",
        "run_dev": "bool"
    }
}